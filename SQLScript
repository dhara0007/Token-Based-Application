Create Database TokenAuthenticationDB
Go


Create table Users
(
     ID int primary key identity,
     FirstName nvarchar(50),
     LastName nvarchar(50),
     Gender nvarchar(50),
     RoleID int
)
Go


Create table Roles
(
     ID int primary key identity,
     Role nvarchar(50)   
)
Go

